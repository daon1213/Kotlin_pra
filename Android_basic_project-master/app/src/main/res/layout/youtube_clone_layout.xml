<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@color/black">

    </TextView>
    <ImageView
        android:layout_margin="15dp"
        android:scaleType="centerCrop"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/youtube">
    </ImageView>
    <TextView
        android:layout_marginLeft="80dp"
        android:layout_marginTop="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:text="Youtube"
        android:textSize="30dp">
    </TextView>
    </FrameLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="70dp"
        android:background="#F5353131">

    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/first">
        <ImageView
            android:id="@+id/DogImage"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            android:scaleType="centerCrop"
            android:layout_width="370dp"
            android:layout_height="300dp"
            android:src="@drawable/dog">


        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="265dp"
            android:layout_marginLeft="310dp"
            android:background="@color/black"
            android:text="10:29"
            android:textColor="@color/white"
            android:textSize="25dp">
        </TextView>

        <RelativeLayout
            android:layout_marginTop="310dp"
            android:layout_width="match_parent"
            android:layout_height="60dp">

        </RelativeLayout>
        <ImageView
            android:layout_marginLeft="20dp"
            android:layout_marginTop="315dp"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/team"
            android:id="@+id/profile">

        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="강아지 너무 귀여붜ㅓㅓ"
            android:textSize="20dp"
            android:layout_marginLeft="15dp"
            android:textColor="@color/white"
            android:layout_alignTop="@id/profile"
            android:layout_toRightOf="@id/profile">

        </TextView>
        <TextView
            android:layout_toRightOf="@id/profile"
            android:layout_marginLeft="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="멍몽이"
            android:layout_alignBottom="@id/profile"
            android:textColor="@color/white"
            android:textSize="15dp">

        </TextView>
    </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/first"
            android:id="@+id/two"
            >
            <ImageView
                android:id="@+id/man"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="20dp"
                android:scaleType="centerCrop"
                android:layout_width="370dp"
                android:layout_height="300dp"
                android:src="@drawable/man">


            </ImageView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="265dp"
                android:layout_marginLeft="310dp"
                android:background="@color/black"
                android:text="08:21"
                android:textColor="@color/white"
                android:textSize="25dp">
            </TextView>

            <RelativeLayout
                android:layout_marginTop="310dp"
                android:layout_width="match_parent"
                android:layout_height="60dp">

            </RelativeLayout>
            <ImageView
                android:layout_marginLeft="20dp"
                android:layout_marginTop="315dp"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@drawable/team"
                android:id="@+id/profile1">

            </ImageView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="난 너무 깐지나"
                android:textSize="20dp"
                android:layout_marginLeft="15dp"
                android:textColor="@color/white"
                android:layout_alignTop="@id/profile1"
                android:layout_toRightOf="@id/profile1">

            </TextView>
            <TextView
                android:layout_toRightOf="@id/profile1"
                android:layout_marginLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="오우오"
                android:layout_alignBottom="@id/profile1"
                android:textColor="@color/white"
                android:textSize="15dp">
            </TextView>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/two"
            android:id="@+id/three">
            <ImageView
                android:id="@+id/man1"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="20dp"
                android:scaleType="centerCrop"
                android:layout_width="370dp"
                android:layout_height="300dp"
                android:src="@drawable/man">


            </ImageView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="265dp"
                android:layout_marginLeft="310dp"
                android:background="@color/black"
                android:text="08:21"
                android:textColor="@color/white"
                android:textSize="25dp">
            </TextView>

            <RelativeLayout
                android:layout_marginTop="310dp"
                android:layout_width="match_parent"
                android:layout_height="60dp">

            </RelativeLayout>
            <ImageView
                android:layout_marginLeft="20dp"
                android:layout_marginTop="315dp"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@drawable/team"
                android:id="@+id/profile2">

            </ImageView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="난 너무 머시써"
                android:textSize="20dp"
                android:layout_marginLeft="15dp"
                android:textColor="@color/white"
                android:layout_alignTop="@id/profile2"
                android:layout_toRightOf="@id/profile2">

            </TextView>
            <TextView
                android:layout_toRightOf="@id/profile2"
                android:layout_marginLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="오우오"
                android:layout_alignBottom="@id/profile2"
                android:textColor="@color/white"
                android:textSize="15dp">
            </TextView>


        </RelativeLayout>
    </RelativeLayout>
    </ScrollView>







</RelativeLayout>